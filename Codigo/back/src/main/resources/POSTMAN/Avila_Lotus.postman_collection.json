				{
					"name": "/auth/login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Successful POST request\", function () {",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
									"});",
									"",
									"const response = pm.response.json();",
									"const token = response.token;",
									"",
									"// Função para decodificar o payload do JWT",
									"function parseJwt (token) {",
									"    const base64Url = token.split('.')[1];",
									"    const base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');",
									"    const jsonPayload = decodeURIComponent(",
									"        atob(base64)",
									"            .split('')",
									"            .map(function(c) {",
									"                return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);",
									"            })",
									"            .join('')",
									"    );",
									"",
									"    return JSON.parse(jsonPayload);",
									"}",
									"",
									"// Decodifica o token",
									"const payload = parseJwt(token);",
									"",
									"// Acessa o campo que indica se é CLIENTE ou PROFISSIONAL",
									"const tipo = payload.tipo; // Certifique-se que no token o campo se chama \"tipo\"",
									"",
									"// Salva o token na variável correta",
									"if (tipo === \"CLIENTE\") {",
									"    pm.environment.set(\"access_token_cliente\", token);",
									"    console.log(\"Token salvo como CLIENTE.\");",
									"} else if (tipo === \"PROFISSIONAL\") {",
									"    pm.environment.set(\"access_token_profissional\", token);",
									"    console.log(\"Token salvo como PROFISSIONAL.\");",
									"} else {",
									"    console.warn(\"Tipo não reconhecido no token:\", tipo);",
									"}",
									"",
									""
								],
